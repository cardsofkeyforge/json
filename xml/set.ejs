<?xml version="1.0" encoding="UTF-8"?>
<cockatrice_carddatabase version="4">
    <sets>
        <set>
            <name><%- expansion.name %></name>
            <longname><%- expansion.longname %></longname>
            <settype>Custom</settype>
            <releasedate><%- expansion.release %></releasedate>
        </set>
    </sets>
    <cards><%cards.forEach(card => {%>
        <card>
            <name><%- card.card_title %><%if (card.card_type === 'Creature1'){%> (1 de 2)<%}else if (card.card_type === 'Creature2'){%> (2 de 2)<%}%></name>
            <text><%- card.card_text.replace(/_x000D_/gi, '\n') %></text><%if (card.card_type === 'Creature' || card.card_type === 'Artifact'){%>
            <cipt>1</cipt><%}%>
            <tablerow><%if (card.card_type === 'Artifact'){%>0<%}else if (card.card_type === 'Upgrade'){%>1<%}else if (card.card_type === 'Action'){%>3<%}else{%>2<%}%></tablerow>
            <set rarity="<%- card.rarity %>" uuid="<%- card.id %>" num="<%- card.card_number %>" muid="<%- expansion.code + '-' + card.card_number %>" picurl="<%- card.front_image %>"><%- expansion.name %></set>
            <prop>
                <layout>normal</layout>
                <side>front</side>
                <type><%- card.traits ? card.traits : card.card_type %></type>
                <maintype><%- card.card_type %></maintype><%if (!card.is_anomaly && !card.is_non_deck){%>
                <colors><%- houses[card.house].name %></colors>
                <coloridentity><%- houses[card.house].name %></coloridentity><%}%><%if (card.card_type === 'Creature' || card.card_type === 'Creature1'){%>
                <pt><%- (card.power ? card.power : 0) + '/' + (card.armor ? card.armor : 0) %></pt><%}%><%if (card.amber > 0){%>
                <loyalty><%- card.amber %></loyalty><%}%>
                <format-standard>ilegal</format-standard>
                <format-commander>ilegal</format-commander>
                <format-modern>ilegal</format-modern>
                <format-pauper>ilegal</format-pauper>
            </prop>
        </card><%})%>
    </cards>
</cockatrice_carddatabase>
